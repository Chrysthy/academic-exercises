Algoritmo "FuncaoExemplo2"

    funcao multiplicacao: inteiro

    var
    aux: inteiro

    inicio
    aux <- a * b

    retorne aux

    fimfuncao

Var
a, b, res: inteiro


Inicio
Escreval ("Digite o primeiro número: ")
Leia(a)

Escreval ("Digite o segundo número: ")
Leia(b)

res <- multiplicacao
Escreval ("A multiplicação dos números é: ", res)
//Escreval(res)


Fimalgoritmo