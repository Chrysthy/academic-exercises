Algoritmo "REPITAATE"

Var
i, num, multiplicacao: inteiro


Inicio

multiplicacao <- 1

REPITA

     escreval ("Digite um número para a multiplicação")

     leia(num)

     multiplicacao <- multiplicacao * num
     
     i <- i + 1
     
ATE  i >= 5
     
     escreval ("A multiplicação é: ", multiplicacao)




Fimalgoritmo